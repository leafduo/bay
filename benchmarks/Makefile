
all: middleware middleware_async

middleware:
	@./run 1 $@
	@./run 5 $@
	@./run 10 $@
	@./run 15 $@
	@./run 20 $@
	@./run 30 $@
	@./run 50 $@
	@./run 100 $@
	@echo

middleware_async:
	@./run 1 $@
	@./run 5 $@
	@./run 10 $@
	@./run 15 $@
	@./run 20 $@
	@./run 30 $@
	@./run 50 $@
	@./run 100 $@
	@echo

.PHONY: all middleware middleware_async